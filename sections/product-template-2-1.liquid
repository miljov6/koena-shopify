              {% else %}
                <button id="unit1" class="btn-unit-1"><p class="btn-title">UNIT PACK</p>{{ product2.price | money }}<span style="text-decoration: line-through; font-size: 11px;"> {{ product2.compare_at_price | money }}</span></button>
