                <button id="unit3" class="btn-unit"><p class="btn-title-selected">3 Pack</p>{{ product.price | money  }}<span style="text-decoration: line-through; font-size: 11px;"> {{ product.compare_at_price | money }}</span></button>
              {% endif %}
              </div>
            {% else %}
              <div id="section---{{section.id}}"style="padding: 20px 0;">
                  <button id="unit1" class="btn-unit"><p class="btn-title-selected">UNIT PACK</p>{{ product.price | money }}<span style="text-decoration: line-through; font-size: 11px;"> {{ product.compare_at_price | money }}</span></button>
