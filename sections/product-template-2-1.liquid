                <button id="unit3" class="btn-unit-1"><p class="btn-title">3 Pack</p>{{ product3.price | money  }}<span style="text-decoration: line-through; font-size: 11px;"> {{ product3.compare_at_price | money }}</span></button>
              {% endif %}
              </div>
